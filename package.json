{
    "name": "godot-launcher",
    "private": true,
    "version": "1.7.0",
    "packageManager": "pnpm@10.23.0",
    "description": "Godot Launcher is a companion app for managing Godot projects with per-project editor settings.",
    "keywords": [
        "godot",
        "launcher",
        "per-project editor settings"
    ],
    "type": "module",
    "main": "dist-electron/main.js",
    "author": {
        "name": "Mario Debono - godotlauncher.org",
        "email": "mario@godotlauncher.org",
        "url": "https://godotlauncher.org"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/godotlauncher/launcher.git"
    },
    "bugs": {
        "url": "https://github.com/godotlauncher/launcher/issues"
    },
    "homepage": "https://godotlauncher.org",
    "engines": {
        "node": ">=22.14.0"
    },
    "scripts": {
        "dev": "npm-run-all --parallel dev:electron dev:react",
        "dev:electron": "pnpm run transpile:electron && cross-env NODE_ENV=development electron .",
        "dev:react": "vite",
        "build": "tsc && vite build",
        "transpile:electron": "tsc --project src/electron/tsconfig.build.json",
        "lint": "biome check",
        "lint:fix": "biome check --write",
        "lint:report": "biome check --reporter=summary --diagnostic-level=error",
        "format": "biome format --write",
        "preview": "vite preview",
        "build:sources": "pnpm run build && pnpm run transpile:electron",
        "dist:mac": "pnpm run build:sources && electron-builder --config electron-builder-config.ts --mac",
        "dist:linux": "pnpm run build:sources && electron-builder --config electron-builder-config.ts --linux",
        "dist:win": "pnpm run build:sources && electron-builder --config electron-builder-config.ts --win ",
        "dist:win:publish": "pnpm run build:sources && electron-builder --config electron-builder-config.ts --win --publish always",
        "dist": "pnpm run build:sources && electron-builder --config electron-builder-config.ts --mac --win --linux",
        "test:e2e": "playwright test",
        "test:unit": "vitest src",
        "app:dir": "electron-builder --dir",
        "app:dist": "electron-builder",
        "postinstall": "electron-builder install-app-deps",
        "changelog": "auto-changelog --ignore-commit-pattern \"[R|r]elease|[c|C]hore|changelog|update changelog|updated changelog|update CHANGELOG\"",
        "doc:screenshots": "playwright test e2e/documentationScreenshot.spec.ts"
    },
    "dependencies": {
        "clsx": "^2.1.1",
        "decompress": "^4.2.1",
        "electron-log": "^5.4.3",
        "electron-updater": "^6.6.2",
        "i18next": "^25.6.3",
        "i18next-fs-backend": "^2.6.1",
        "javascript-time-ago": "^2.5.12",
        "lucide-react": "^0.554.0",
        "mustache": "^4.2.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-i18next": "^16.3.4",
        "semver": "^7.7.3",
        "which": "^6.0.0"
    },
    "devDependencies": {
        "@babel/helpers": "^7.28.4",
        "@biomejs/biome": "^2.3.6",
        "@playwright/test": "^1.56.1",
        "@tailwindcss/postcss": "^4.1.17",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.17",
        "@types/decompress": "^4.2.7",
        "@types/mustache": "^4.2.6",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.6",
        "@types/react-dom": "^19.2.3",
        "@types/semver": "^7.7.1",
        "@types/which": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "@vitejs/plugin-react": "^5.1.1",
        "@vitest/coverage-v8": "^4.0.10",
        "auto-changelog": "^2.5.0",
        "cross-env": "^10.1.0",
        "daisyui": "^5.5.5",
        "electron": "^39.2.2",
        "electron-builder": "^26.1.0",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.17",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vitest": "^4.0.10"
    }
}